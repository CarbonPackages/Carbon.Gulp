{
  "name": "gulpfile",
  "version": "5.0.0",
  "description": "Carbon/Gulp is a delicious blend of tasks and build tools poured into Gulp to form a full-featured modern asset pipeline for Flow Framework and Neos CMS. Based on https://github.com/jonnitto/gulpfile.js",
  "private": true,
  "author": "Jon Uhlmann",
  "license": "MIT",
  "keywords": [
    "gulp",
    "neoscms",
    "flow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jonnitto/Carbon.Gulp.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Explorer > 10"
  ],
  "prettier": {
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "*.scss",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "*.js",
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "scripts": {
    "gulp": "gulp --cwd ./ --gulpfile ./Build/Gulp",
    "tasks": "yarn gulp -T",
    "start": "yarn;yarn gulp",
    "debug": "yarn gulp --debug",
    "beautify": "yarn gulp --beautify",
    "nomaps": "yarn gulp --nomaps",
    "build": "yarn gulp build",
    "pipeline": "yarn gulp pipeline",
    "css": "yarn gulp css",
    "scss": "yarn gulp scss",
    "lint": "yarn gulp lint",
    "js": "yarn gulp js",
    "optimizeImages": "yarn gulp optimizeImages",
    "optimizeSvg": "yarn gulp optimizeSvg",
    "compress": "yarn gulp compress",
    "showConfig": "yarn gulp showConfig"
  },
  "dependencies": {},
  "devDependencies": {
    "ansi-colors": "^1.0.1",
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "2.19.0",
    "caniuse-db": "^1.0.30000784",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-prettier": "^2.4.0",
    "fancy-log": "^1.3.2",
    "fs-readdir-recursive": "^1.1.0",
    "glob": "^7.1.2",
    "gulp": "next",
    "gulp-brotli": "^1.2.1",
    "gulp-changed": "^3.1.1",
    "gulp-chmod": "^2.0.0",
    "gulp-cssbeautify": "^1.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-header": "^1.8.9",
    "gulp-imagemin": "^4.0.0",
    "gulp-memory-cache": "^0.3.0",
    "gulp-noop": "^1.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylelint": "^6.0.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "gulp-zopfli": "^1.0.0",
    "merge-stream": "^1.0.1",
    "node-notifier": "^5.1.2",
    "object-assign-deep": "^0.3.1",
    "pleeease-filters": "^4.0.0",
    "postcss": "^6.0.15",
    "postcss-animations": "^6.0.2",
    "postcss-assets": "^5.0.0",
    "postcss-center": "^1.1.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-flexbox": "^1.0.3",
    "postcss-magic-animations": "^0.3.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-quantity-queries": "^0.5.0",
    "postcss-reporter": "^5.0.0",
    "postcss-round-subpixels": "^1.2.0",
    "postcss-rtl": "^1.1.3",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "postcss-short": "^4.1.0",
    "postcss-vmax": "^1.0.0",
    "prettier": "^1.9.2",
    "prettyjson": "^1.2.1",
    "rollup": "^0.53.2",
    "rollup-plugin-amd": "^2.0.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rucksack-css": "^1.0.2",
    "sort-css-media-queries": "^1.3.4",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.2.0",
    "text-table": "^0.2.0"
  }
}
