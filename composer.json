{
    "name": "carbon/gulp",
    "type": "neos-build",
    "description":
        "Carbon/Gulp is a delicious blend of tasks and build tools poured into Gulp to form a full-featured modern asset pipeline for Flow Framework and Neos CMS.",
    "license": "MIT",
    "require": {
        "neos/composer-plugin": "^1.0.6|^2.0.0"
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitignore",
            ".github",
            ".gitattributes",
            "CODE_OF_CONDUCT.md",
            "PITCHME.md",
            "PITCHME.yaml",
            "assets/Pitchme"
        ]
    },
    "extra": {
        "installer-name": "Gulp"
    }
}
