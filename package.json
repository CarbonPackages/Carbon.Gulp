{
  "name": "Carbon.Gulp",
  "version": "6.0.0",
  "description": "Carbon/Gulp is a delicious blend of tasks and build tools poured into Gulp to form a full-featured modern asset pipeline for Flow Framework and Neos CMS.",
  "private": true,
  "author": "Jon Uhlmann <me@jonnitto.ch> (https://jonnitto.ch)",
  "bugs": "https://github.com/CarbonPackages/Carbon.Gulp/issues",
  "homepage": "https://github.com/CarbonPackages/Carbon.Gulp",
  "license": "MIT",
  "keywords": [
    "gulp",
    "neoscms",
    "flow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CarbonPackages/Carbon.Gulp.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not ie <= 10",
    "not ie_mob <= 10",
    "not bb <= 10",
    "not op_mob <= 12.1"
  ],
  "prettier": {
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "*.scss",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "*.js",
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "scripts": {
    "gulp": "gulp --cwd ./ --gulpfile ./Build/Gulp",
    "tasks": "yarn gulp -T",
    "start": "yarn;yarn gulp",
    "debug": "yarn gulp --debug",
    "beautify": "yarn gulp --beautify",
    "nomaps": "yarn gulp --nomaps",
    "build": "yarn;yarn gulp build",
    "watch": "yarn;yarn gulp watch",
    "pipeline": "yarn;yarn gulp pipeline",
    "css": "yarn gulp css",
    "scss": "yarn gulp scss",
    "js": "yarn gulp js",
    "optimizeImages": "yarn gulp optimizeImages",
    "optimizeSvg": "yarn gulp optimizeSvg",
    "showConfig": "yarn gulp showConfig"
  },
  "dependencies": {},
  "devDependencies": {
    "ansi-colors": "^2.0.5",
    "autoprefixer": "^9.0.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "2.24.6",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.0.3",
    "del": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.23.0",
    "eslint-plugin-prettier": "^2.6.2",
    "fancy-log": "^1.3.2",
    "fs-readdir-recursive": "^1.1.0",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-chmod": "^2.0.0",
    "gulp-cssbeautify": "^1.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-header": "^2.0.5",
    "gulp-imagemin": "^4.1.0",
    "gulp-memory-cache": "^0.3.0",
    "gulp-noop": "^1.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.3.0",
    "gulp-rollup-each": "^2.1.1",
    "gulp-sass": "^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-svgmin": "^1.2.4",
    "merge-stream": "^1.0.1",
    "node-notifier": "^5.2.1",
    "node-sass-tilde-importer": "^1.0.2",
    "object-assign-deep": "^0.4.0",
    "pleeease-filters": "^4.0.0",
    "postcss": "^7.0.1",
    "postcss-assets": "^5.0.0",
    "postcss-center": "^1.1.0",
    "postcss-fixes": "^2.0.1",
    "postcss-flexbox": "^1.0.3",
    "postcss-focus": "^3.0.0",
    "postcss-momentum-scrolling": "^1.4.8",
    "postcss-preset-env": "^5.2.3",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-quantity-queries": "^0.5.0",
    "postcss-reporter": "^5.0.0",
    "postcss-round-subpixels": "^1.2.0",
    "postcss-rtl": "^1.3.1",
    "postcss-short": "^4.1.0",
    "postcss-vmax": "^1.0.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "prettier-stylelint": "^0.4.2",
    "prettyjson": "^1.2.1",
    "rollup": "^0.63.4",
    "rollup-plugin-amd": "^3.0.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^1.0.1",
    "rucksack-css": "^1.0.2",
    "sort-css-media-queries": "^1.3.4",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.2.0",
    "text-table": "^0.2.0"
  }
}
